{
  "name": "fabric-vault",
  "displayName": "FabricVault",
  "version": "0.0.1",
  "description": "A secure browser extension wallet for Hyperledger Fabric",
  "author": "ifeanyi ugwu <valentineifeanyiugwu@outlook.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@noble/curves": "^1.9.0",
    "@plasmohq/messaging": "^0.7.1",
    "@tanstack/react-router": "^1.117.1",
    "@tanstack/router": "0.0.1-beta.53",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*",
      "http://*/*",
      "ws://*/*",
      "wss://*/*"
    ],
    "permissions": [
      "storage",
      "tabs"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "injected.js"
        ],
        "matches": [
          "https://*/*",
          "http://*/*"
        ]
      }
    ]
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af"
}
